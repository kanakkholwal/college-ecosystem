{
  "name": "platform",
  "version": "0.8.0",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "start": "next start",
    "dev": "next dev --turbo",
    "build": "next build",
    "db:push": "drizzle-kit push",
    "db:push:dev": "cross-env NODE_ENV=development drizzle-kit push",
    "db:push:prod": "cross-env NODE_ENV=production drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:pull:dev": "cross-env NODE_ENV=development drizzle-kit pull",
    "db:pull:prod": "cross-env NODE_ENV=production drizzle-kit pull",
    "db:generate": "drizzle-kit generate",
    "db:generate:dev": "cross-env NODE_ENV=development drizzle-kit generate",
    "db:generate:prod": "cross-env NODE_ENV=production drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:dev": "cross-env NODE_ENV=development drizzle-kit migrate",
    "db:migrate:prod": "cross-env NODE_ENV=production drizzle-kit migrate",
    "format": "npx prettier --write \"**/*.{ts,tsx,js,jsx,md}\"",
    "build:no-warn": "next build --experimental-build-mode compile",
    "build:isolated": "docker build -t platform .",
    "build:prod": "npx opennext:build && wrangler deploy --dry-run --outdir=dist",
    "opennext:build": "opennextjs-cloudflare build",
    "opennext:deploy": "opennextjs-cloudflare deploy -- --keep-vars",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
