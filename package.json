{
  "name": "college-ecosystem",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md}\"",
    "dev:server": "go run ./apps/server/*.go",
    "build:server": "go build -o server go run ./apps/server/*.go"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.0.5",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.5.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
